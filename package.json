{
  "name": "ngx-swiper-wrapper",
  "version": "4.6.6",
  "private": true,
  "license": "MIT",
  "scripts": {
    "init": "npm install",
    "start": "npm run build",
    "watch": "watch 'npm run build' src/",
    "clean": "rm -rf dist bundles node_modules",
    "link": "ngm link -p src",
    "lint": "npm run lint:js && npm run lint:scss",
    "build": "ngm build -p src --clean --skip-bundles",
    "deploy": "npm run build && npm run inline && npm publish",
    "inline": "ng2-inline --compress -b dist/lib/ -o ./ 'dist/lib/*.js'",
    "lint:ts": "tslint 'src/**/*.ts'",
    "lint:scss": "stylelint 'src/**/*.scss' --syntax scss"
  },
  "dependencies": {
    "swiper": "^3.0.0"
  },
  "devDependencies": {
    "@angular/common": "4.4.3",
    "@angular/compiler": "4.4.3",
    "@angular/compiler-cli": "4.4.3",
    "@angular/core": "4.4.3",
    "@angular/platform-browser": "4.4.3",
    "@types/node": "~6.0.0",
    "@types/swiper": "~2.0.0",
    "angular2-inline-template-style": "~1.0.0",
    "codelyzer": "3.2.1",
    "core-js": "2.4.1",
    "ngm-cli": "0.8.3",
    "node-sass": "4.5.3",
    "rxjs": "5.4.3",
    "stylelint": "~8.0.0",
    "stylelint-config-standard": "~17.0.0",
    "stylelint-order": "~0.6.0",
    "tslint": "5.7.0",
    "typescript": "2.4.2",
    "watch": "~1.0.0",
    "zone.js": "0.8.16"
  },
  "peerDependencies": {
    "@angular/common": ">=4.0.0",
    "@angular/core": ">=4.0.0"
  }
}
